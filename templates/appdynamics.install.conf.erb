<!-- This configuration is used for unatteneded installation of AppDynamics -->
<!-- It contains installation folder/permissions and AppDynamics connection --> 
<winston>
  <logFileDirectory directory="<%=@appdynamics_logdir%>" />
  <logFileFolderAccessPermissions defaultAccountsEnabled="false">
    <account name="NT AUTHORITY\LOCAL SERVICE" displayName="LOCAL SERVICE" />
    <account name="NT AUTHORITY\SYSTEM" displayName="SYSTEM" />
    <account name="NT AUTHORITY\NETWORK SERVICE" displayName="NETWORK SERVICE" />
    <account name="IIS_IUSRS" displayName="ApplicationPool Identity" />
  </logFileFolderAccessPermissions>
  <appdynamics-agent xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <controller host="<%=@appdynamics_controllerhost%>" port="<%=@appdynamics_controllerport%>" ssl="<%=@appdynamics_ssl%>">
      <application name="<%=@appdynamics_applicationame%>" />
    </controller>
    <machine-agent />
  </appdynamics-agent>
</winston>